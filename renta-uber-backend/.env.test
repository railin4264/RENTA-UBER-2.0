# Test Environment Configuration
NODE_ENV=test

# Test Database
TEST_DATABASE_URL="postgresql://username:password@localhost:5432/renta_uber_test"

# Test JWT
JWT_SECRET="test-jwt-secret-key-for-testing-only"
JWT_EXPIRES_IN="1h"

# Test Server
